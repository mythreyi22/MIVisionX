import vx_rpp

# create input and output images
data input  = image:480,360,U008
data output = image:480,360,U008

# specify input source for input image and request for displaying input and output images
read input  ../images/lady.png
view input  inputWindow
view output contrast_8

# compute contrast for image by calling contrast kernel
data min = scalar:UINT32,0
data max = scalar:UINT32,255
node org.rpp.Contrast input output min max
